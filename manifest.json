{
  "name": "Tabs",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Extension to manage tabs and windows",
  "permissions": [
    "tabs",
    "storage",
    "tabGroups"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "48": "icon.png"
    }
  },
  "icons": {
    "48": "icon.png"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}